#!/usr/bin/env bash
echo -e 'OK' | rofi -p "prompt" -dmenu -mesg 'PRIMARY COMMANDS:&#x0a;Mod+P: Logout/Restart&#x0a;Mod+L: Lock Screen&#x0a;Mod+C: Chrome Browser&#x0a;Mod+X: Terminal&#x0a;Mod+PrtScr: Screenshot&#x0a;Mod+D: Command Launch Menu&#x0a;Mod+Q: Kill Focused Window&#x0a;Mod+Shift+F: Global Find&#x0a;Mod+Shift+X: Kill WINE window&#x0a;Mod+W: Change Wallpaper&#x0a;Mod+Shift+W: Change Wallpaper (special)&#x0a;&#x0a;FOCUS/NAV COMMANDS:&#x0a;Mod+Up,Down,Left,Right: Switch Focus&#x0a;Mod+Shift+Up,Down,Left,Right: Move Focused Window&#x0a;Mod+(0-9): Move to workspace #(0-9)&#x0a;Mod+R: Resize Focused Window&#x0a;Mod+S: Stacking Layout&#x0a;Mod+T: Tabbed Layout&#x0a;Mod+E: Split Layout (Default)&#x0a;&#x0a;EXIT/RELOAD COMMANDS:&#x0a;Mod+Shift+C: Reload Config&#x0a;Mod+Shift+R: Restart i3&#x0a;Mod+Shift+E: Exit Session'
